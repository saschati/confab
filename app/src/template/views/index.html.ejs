            <div class="stream__tv tv">
                <span class="tv__label mark mark_type_icon mark_color_crayola">
                    <i class="mark__icon-before icon icon_stream"></i>
                    LIVE
                </span>
                <div class="tv__toolbar toolbar cursor cursor_type_pointing">
                    <button class="toolbar__item">
                        <i class="icon icon_volume_up"></i>
                    </button>
                    <button class="toolbar__item">
                        <i class="icon icon_volume_off"></i>
                    </button>
                    <button class="toolbar__item">
                        <i class="icon icon_pause"></i>
                    </button>
                    <button class="toolbar__item">
                        <i class="icon icon_subtitles"></i>
                    </button>
                    <button class="toolbar__item">
                        <i class="icon icon_fullscreen"></i>
